{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build_creative",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/CreativeMode/CreativeMode.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:Summary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish_creative",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/CreativeMode/CreativeMode.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch_creative",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/CreativeMode/CreativeMode.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
    ]
}